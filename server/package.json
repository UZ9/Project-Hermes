{
  "name": "project-hermes-backend",
  "version": "0.3.0",
  "description": "The backend portion of Project Hermes",
  "engines": {
    "node": "14.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "tsc-watch --onSuccess \"npm start\"",
    "test": "node test.js"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [
      "*.test.ts",
      "db/*"
    ],
    "delay": "2"
  },
  "dependencies": {
    "@types/cli-progress": "^3.11.0",
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "js-yaml": "^4.1.0",
    "mongodb": "^4.9.0",
    "node-fetch": "^3.2.4",
    "passport-auth-token": "^1.0.1",
    "path": "^0.12.7",
    "robotevents": "^2.0.1",
    "socket.io": "^4.5.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/winston": "^2.4.4",
    "cli-progress": "^3.11.1",
    "got": "^12.1.0",
    "tape": "^5.5.3",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
